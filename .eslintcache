       </TableHead>\n\n          <TableBody>\n            {props.data &&\n              props.data.map((row) => (\n                <StyledTableRow key={row._id}>\n                  <StyledTableCell align=\"left\"> {row._id} </StyledTableCell>\n                  <StyledTableCell>\n                    {getWinnerStatus(row)}\n                  </StyledTableCell>\n                  <StyledTableCell>\n                    {row.status === 1\n                      ? row.status === 2\n                        ? \"Playing\"\n                        : \"Finished\"\n                      : \"Waiting\"}\n                  </StyledTableCell>\n                  <StyledTableCell>\n                    {row.history.length}\n                  </StyledTableCell>\n                  <StyledTableCell>\n                    {row.chat.length}\n                  </StyledTableCell>\n                  <StyledTableCell align=\"right\">\n                    <Button onClick={() => handleClick(row)}>\n                      <VisibilityIcon />\n                    </Button>\n                  </StyledTableCell>\n                </StyledTableRow>\n              ))}\n          </TableBody>\n        </Table>\n      )}\n    </div>\n  );\n}\n","/home/ubuntu/Desktop/hk1/advance_web/web_final_caro/caro-client-react/src/components/UserOnline.js",["250","251"],"import React, { useState, useEffect } from 'react';\nimport CssBaseline from '@material-ui/core/CssBaseline';\nimport Grid from '@material-ui/core/Grid';\nimport Typography from '@material-ui/core/Typography';\nimport { makeStyles } from '@material-ui/core/styles';\nimport Container from '@material-ui/core/Container';\nimport { useHistory } from 'react-router-dom';\nimport Button from \"@material-ui/core/Button\";\nimport {ioClient} from '../socket/index';\nimport{getUserOnlineSock} from '../socket/HomeSocket';\nconst useStyles = makeStyles((theme) => ({\n    paper: {\n        marginTop: theme.spacing(8),\n        display: 'flex',\n        flexDirection